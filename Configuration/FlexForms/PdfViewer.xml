<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sPARAMS>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:pdfjs/Resources/Private/Language/locallangFlexforms.xlf:pdfViewer.sheetTitle</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <switchableControllerActions>
                        <TCEforms>
                            <label>LLL:EXT:pdfjs/Resources/Private/Language/locallangFlexforms.xlf:pdfViewer.layout</label>
                            <onChange>reload</onChange>
                            <config>
                                <type>select</type>
                                <items type="array">
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">LLL:EXT:pdfjs/Resources/Private/Language/locallangFlexforms.xlf:pdfViewer.layout.simple</numIndex>
                                        <numIndex index="1">PdfViewer->showSimple</numIndex>
                                    </numIndex>
                                    <!--
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">LLL:EXT:pdfjs/Resources/Private/Language/locallangFlexforms.xlf:pdfViewer.layout.complete</numIndex>
                                        <numIndex index="1">PdfViewer->showComplete</numIndex>
                                    </numIndex>
                                    -->
                                </items>
                            </config>
                        </TCEforms>
                    </switchableControllerActions>

                    <settings.file>
                        <TCEforms>
                            <label>LLL:EXT:pdfjs/Resources/Private/Language/locallangFlexforms.xlf:pdfViewer.file</label>
                            <config>
                                <type>inline</type>
                                <appearance type="array">
                                    <createNewRelationLinkTitle>LLL:EXT:cms/locallang_ttc.xlf:media.addFileReference</createNewRelationLinkTitle>
                                    <enabledControls type="array">
                                        <delete>1</delete>
                                        <dragdrop>1</dragdrop>
                                        <hide>1</hide>
                                        <info>1</info>
                                        <localize>1</localize>
                                        <sort>0</sort>
                                    </enabledControls>
                                    <headerThumbnail type="array">
                                        <field>uid_local</field>
                                        <height>45c</height>
                                        <width>45</width>
                                    </headerThumbnail>
                                    <useSortable>0</useSortable>
                                </appearance>
                                <foreign_types>
                                    <numIndex index="5">
                                        <showitem>--palette--;LLL:EXT:lang/locallang_tca.xlf:sys_file_reference.imageoverlayPalette;imageoverlayPalette,--palette--;;filePalette</showitem>
                                    </numIndex>
                                </foreign_types>
                                <behaviour type="array">
                                    <localizationMode>select</localizationMode>
                                    <localizeChildrenAtParentLocalization>1</localizeChildrenAtParentLocalization>
                                </behaviour>
                                <filter type="array">

                                </filter>
                                <foreign_field>uid_foreign</foreign_field>
                                <foreign_label>uid_local</foreign_label>
                                <foreign_match_fields type="array">
                                    <fieldname>file</fieldname>
                                </foreign_match_fields>
                                <foreign_selector>uid_local</foreign_selector>
                                <foreign_selector_fieldTcaOverride type="array">
                                    <config type="array">
                                        <appearance type="array">
                                            <elementBrowserAllowed>pdf</elementBrowserAllowed>
                                            <elementBrowserType>file</elementBrowserType>
                                        </appearance>
                                    </config>
                                </foreign_selector_fieldTcaOverride>
                                <foreign_sortby>sorting_foreign</foreign_sortby>
                                <foreign_table>sys_file_reference</foreign_table>
                                <foreign_table_field>tablenames</foreign_table_field>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                            </config>
                        </TCEforms>
                    </settings.file>

                    <settings.flexform.maxWidth>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:pdfjs/Resources/Private/Language/locallangFlexforms.xlf:pdfViewer.maxWidth</label>
                            <config>
                                <type>input</type>
                                <size>5</size>
                                <max>6</max>
                                <eval>trim</eval>
                            </config>
                        </TCEforms>
                    </settings.flexform.maxWidth>

                    <settings.flexform.imgNavigationPos>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:pdfjs/Resources/Private/Language/locallangFlexforms.xlf:pdfViewer.imgNavigationPos</label>
                            <displayCond>FIELD:switchableControllerActions:=:PdfViewer->showSimple</displayCond>
                            <config>
                                <type>select</type>
                                <items type="array">
                                    <numIndex index="0">
                                        <numIndex index="0">LLL:EXT:pdfjs/Resources/Private/Language/locallangFlexforms.xlf:pdfViewer.imgNavigationPos.ts</numIndex>
                                        <numIndex index="1">ts</numIndex>
                                    </numIndex>
                                    <numIndex index="1">
                                        <numIndex index="0">LLL:EXT:pdfjs/Resources/Private/Language/locallangFlexforms.xlf:pdfViewer.imgNavigationPos.disable</numIndex>
                                        <numIndex index="1">disabled</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">LLL:EXT:pdfjs/Resources/Private/Language/locallangFlexforms.xlf:pdfViewer.imgNavigationPos.top</numIndex>
                                        <numIndex index="1">top</numIndex>
                                    </numIndex>
                                    <numIndex index="3">
                                        <numIndex index="0">LLL:EXT:pdfjs/Resources/Private/Language/locallangFlexforms.xlf:pdfViewer.imgNavigationPos.right</numIndex>
                                        <numIndex index="1">right</numIndex>
                                    </numIndex>
                                    <numIndex index="4">
                                        <numIndex index="0">LLL:EXT:pdfjs/Resources/Private/Language/locallangFlexforms.xlf:pdfViewer.imgNavigationPos.bottom</numIndex>
                                        <numIndex index="1">bottom</numIndex>
                                    </numIndex>
                                    <numIndex index="5">
                                        <numIndex index="0">LLL:EXT:pdfjs/Resources/Private/Language/locallangFlexforms.xlf:pdfViewer.imgNavigationPos.left</numIndex>
                                        <numIndex index="1">left</numIndex>
                                    </numIndex>
                                </items>
                                <default>ts</default>
                            </config>
                        </TCEforms>
                    </settings.flexform.imgNavigationPos>

                    <settings.flexform.hideDownloadLink>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:pdfjs/Resources/Private/Language/locallangFlexforms.xlf:pdfViewer.hideDownloadLink</label>
                            <config>
                                <type>check</type>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </settings.flexform.hideDownloadLink>
                </el>
            </ROOT>
        </sPARAMS>
    </sheets>
</T3DataStructure>